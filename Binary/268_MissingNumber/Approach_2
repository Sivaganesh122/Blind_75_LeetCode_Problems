class Solution {
    public int missingNumber(int[] nums) {
        int xor1 =0;
        for(int i = 0; i<=nums.length; i++){
            xor1 ^= i;
        }
        int xor2 = 0;
        for(int n: nums){
            xor2 ^= n;
        }
        return xor1^xor2;
    }
}
//Time Complexity: O(n)
//Space Complexity: O(1)
