class Solution {
    public ListNode reverseList(ListNode head) {
        if(head == null || head.next == null)
            return head;
        Stack<Integer> s = new Stack<>();
        ListNode temp = head;
        while(temp!=null){
            s.push(temp.val);
            temp = temp.next;
        }
        temp  = head;
        while(!s.isEmpty()){
            temp.val = s.pop();
            temp = temp.next;
        }
        return head;
    }
}
//Time Complexity: O(n)
//Space Complexity: O(n)
