public class Solution {
    public boolean hasCycle(ListNode head) {
        if(head == null){
            return false;
        }
        Set<ListNode> set = new HashSet<>();
        ListNode temp = head;
        while(temp != null){
            if(!set.add(temp))
                return true;
            temp = temp.next;
        }
        return false;
    }
}
//Time Complexity: O(n)
//Space Complexity: O(n)
