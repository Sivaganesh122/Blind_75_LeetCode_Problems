class Solution {
    public ListNode removeNthFromEnd(ListNode head, int n) {
        if(head == null || n<=0){
            return head;
        }
        ListNode dummy = new ListNode(-1, head);
        ListNode slow = dummy;
        ListNode fast = dummy;

        for(int i = 0; i<=n; i++){
            if(fast != null)
                fast = fast.next;
        }

        while(fast!=null ){
            slow = slow.next;
            fast = fast.next;
        }

        slow.next = slow.next.next;
        return dummy.next;
    }
}
