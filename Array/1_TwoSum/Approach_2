//Two Sum 
class Solution {
    public int[] twoSum(int[] nums, int target) {
        Map<Integer,Integer> map = new HashMap<>();
        for(int i = 0; i<nums.length; i++){
            int bal = target - nums[i];
            if(map.containsKey(bal))
                return new int[]{i,map.get(bal)};
            map.put(nums[i],i);
        }
        return new int[]{};
    }
}
//Time Complexity: O(n)
//Space Complexity: O(n)
