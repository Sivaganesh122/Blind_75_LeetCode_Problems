class Solution {
    public int maxArea(int[] height) {
        int max = 0;
        int n = height.length;

        for (int i = 0; i < n; i++) {
            for (int j = i + 1; j < n; j++) {
                int area = Math.min(height[i], height[j]) * (j - i);
                max = Math.max(max, area);
            }
        }
        return max;
    }
}
//Time Complexity: O(nÂ²)
//Space Complexity: O(1)

//Time Limit Exceeded  (59 / 65 testcases passed)
